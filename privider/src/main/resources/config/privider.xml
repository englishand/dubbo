<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--配置应用服务名，用于计算依赖关系-->
    <dubbo:application name="privider"/>
    <!--使用zookeeper注册中心，使服务注册在注册中心,集群部署用逗号分割-->
    <dubbo:registry address="zookeeper://127.0.0.1:2181?backup=192.168.110.182:2181,127.0.0.1:2181"/>
    <!--或使用如下配置注册中心-->
<!--    <dubbo:registry protocol="zookeeper" address="127.0.0.1:2181,192.168.110.182:2181"/>-->
    <!--配置扫描包，暴露服务接口-->
    <dubbo:annotation package="com.zhy.service.impl"/>

</beans>